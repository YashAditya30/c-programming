#include<stdio.h>
int main()
{
    int sp,cp;
    printf("enter sp");
    scanf("%d",&sp);
    printf("enter cp");
    scanf("%d",&cp);
    if(sp<cp)
        printf("the seller has made a loss of %d", cp-sp);
    if(sp>cp)
        printf("the seller has made a profit of %d", sp-cp);
    if(cp==sp)
    printf("no profit no loss");
    return 0;
}
